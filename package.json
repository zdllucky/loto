{
  "name": "keystone-app",
  "version": "1.0.2",
  "private": true,
  "scripts": {
    "dev": "COOKIE_SECRET=f7401e2ec9452dc942a52c8f45f2d7c5424c37f92c85afb24ed6470580d1c05c keystone dev --with-migrations",
    "start": "keystone start",
    "migrate_start": "npm run migrate && npm run start",
    "build": "keystone build",
    "make_migrations": "keystone prisma migrate dev --create-only",
    "migrate_dev": "keystone prisma migrate dev",
    "migrate": "keystone prisma migrate deploy",
    "postinstall": "keystone build --no-ui --frozen && keystone telemetry disable"
  },
  "dependencies": {
    "@bull-board/api": "^5.2.1",
    "@bull-board/express": "^5.2.1",
    "@keystone-6/auth": "^7.0.0",
    "@keystone-6/core": "^5.0.0",
    "@keystone-6/fields-document": "^8.0.0",
    "@paralleldrive/cuid2": "^2.2.1",
    "@redis/client": "^1.5.8",
    "bullmq": "^3.15.5",
    "express": "^4.18.2",
    "express-zod-api": "^10.8.1",
    "swagger-ui-express": "^4.6.3",
    "typescript": "^4.9.5",
    "unique-username-generator": "^1.1.3",
    "winston": "^3.9.0",
    "zod": "^3.21.4",
    "zod-prisma": "^0.5.4"
  },
  "devDependencies": {
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-unicorn": "^47.0.0",
    "prettier": "^2.8.8"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "@typescript-eslint/no-non-null-assertion": "off",
      "@typescript-eslint/ban-ts-comment": "off",
      "@typescript-eslint/no-explicit-any": "off",
      "import/no-unresolved": "off"
    },
    "extends": [
      "eslint:recommended",
      "plugin:import/recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "sourceType": "module"
    },
    "root": true,
    "plugins": [
      "import",
      "eslint-plugin-prefer-arrow",
      "eslint-plugin-unicorn",
      "eslint-plugin-react",
      "@typescript-eslint"
    ],
    "ignorePatterns": [
      "**/*.d.ts",
      "**/node_modules/**"
    ]
  }
}
